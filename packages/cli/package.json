{
  "name": "@ethui/stacks",
  "version": "0.1.0",
  "description": "CLI for authenticating with ethui stacks",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "stacks": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ethui/stacks.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "lint": "biome lint src/",
    "format": "biome format src/ --write",
    "check": "biome check src/",
    "prepublishOnly": "pnpm run build",
    "publish": "pnpm publish --access public"
  },
  "keywords": [
    "ethui",
    "stacks",
    "cli",
    "authentication"
  ],
  "author": "ethui",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
